set -ex
( export PATH="$PATH:$(npm bin)"
  cd angular2-client
  npm install typescript@^1.5.0-beta
  npm install tsd
  tsd query angular2 --action install
  tsd install es6-promise --save
  tsd install rx --save
  tsd install rx-lite --save
)
( cd jdbc-auth-service
  mvn clean install
)
( cd data-initializer-service
  mvn clean install
)
( cd task-service
  mvn clean install
)
